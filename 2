let a=11;

switch(a){
   case 1:
    console.log("Dushanba"); break;
    case 2:
        console.log("seshanba");break;
    case 3:
        console.log("Chorshanba");break;
    case 4:
        console.log("Payshanba");break;
    case 5:
        console.log("Juma");break;
    case 6:
        console.log("shanba");break;
    default:
        console.log("none");
}
